    <div class="field">
      <%= f.label :client_part_nr %><br>
      <%= f.text_field :client_part_nr %>
    </div>
    <%= f.hidden_field :client_tenant_id, value: @tenant.id %>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>
