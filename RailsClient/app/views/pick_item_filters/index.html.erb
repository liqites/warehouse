<h1>Listing pick_item_filters</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>User</th>
      <th>Value</th>
      <th>Filterable</th>
      <th>Filterable type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pick_item_filters.each do |pick_item_filter| %>
      <tr>
        <td><%= pick_item_filter.id %></td>
        <td><%= pick_item_filter.user_id %></td>
        <td><%= pick_item_filter.value %></td>
        <td><%= pick_item_filter.filterable_id %></td>
        <td><%= pick_item_filter.filterable_type %></td>
        <td><%= link_to 'Show', pick_item_filter %></td>
        <td><%= link_to 'Edit', edit_pick_item_filter_path(pick_item_filter) %></td>
        <td><%= link_to 'Destroy', pick_item_filter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pick item filter', new_pick_item_filter_path %>
