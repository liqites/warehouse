<table class="table table-striped table-bordered table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>零件号</th>
    <th>入库数量</th>
    <th>报废数量</th>
    <th>实际使用量</th>
  </tr>
  </thead>

  <tbody>
  <%@movements.each_with_index do |m,index|%>
      <tr>
        <td><%= index+1%></td>
        <td><%= m['partNr']%></td>
        <td><%= m['src_qty']%></td>
        <td><%= m['dse_qty']%></td>
        <td><%= (m['src_qty'] -m['dse_qty']) %></td>
      </tr>
  <%end%>
  </tbody>
</table>
