<h2><%= @location.name%>仓库列表</h2>
<br/>
<div class="digg_pagination">
  <div class="page_info">
    <%= page_entries_info @whouses %>
  </div>
  <%= will_paginate @whouses, :container => false %>
</div>
<table class="table table-bordered table-striped globle-table table-hover">
  <thead>
  <tr>
    <td>NO.</td>
    <td>编号</td>
    <td>名称</td>
  </tr>
  </thead>
  <tbody>
  <% @whouses.each_with_index do |w,index|%>
      <tr>
        <td><%= index+1+@whouses.offset%></td>
        <td><%= w.id %></td>
        <td><%= w.name %></td>
      </tr>
  <% end%>
  </tbody>
</table>
<div class="digg_pagination">
  <%= will_paginate @whouses, :container => false %>
</div>
<%= link_to '返回', locations_path ,:class => "btn btn-default btn-lg" %>