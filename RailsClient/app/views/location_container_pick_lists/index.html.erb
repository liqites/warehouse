<h1>Listing location_container_pick_lists</h1>

<table>
  <thead>
    <tr>
      <th>Location container</th>
      <th>Pick list</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_container_pick_lists.each do |location_container_pick_list| %>
      <tr>
        <td><%= location_container_pick_list.location_container_id %></td>
        <td><%= location_container_pick_list.pick_list_id %></td>
        <td><%= link_to 'Show', location_container_pick_list %></td>
        <td><%= link_to 'Edit', edit_location_container_pick_list_path(location_container_pick_list) %></td>
        <td><%= link_to 'Destroy', location_container_pick_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location container pick list', new_location_container_pick_list_path %>
