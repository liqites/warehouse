<table class="table table-striped table-condensed table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>唯一码</th>
    <th>零件号</th>
    <th>描述</th>
    <th>数量</th>
    <!--<th>箱数</th>-->
    <th>仓库</th>
    <!--<th>状态</th>-->

  </tr>
  </thead>
  <tbody>
  <% @movements.each_with_index do |p, index| %>
      <tr>
        <td><%= index+1 %></td>
        <td>
          <% if @commit_value == "详细" %>
              <%= p.packageId %>
          <% else %>
              <%= %>
          <% end %>
        </td>
        <td><%= p.partNr %></td>
        <td><%= p.part.blank? ? '' : p.part.description %></td>
        <td><%= p.total_qty %></td>
        <!--<td><%= p['box'] %></td>-->
        <td><%= p.wh_id %></td>
        <!--<td><%= MovableState.display(p['state']) %></td>-->

      </tr>
  <% end %>
  </tbody>
</table>
