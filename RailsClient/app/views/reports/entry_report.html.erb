<h2>入库报表</h2>
<br/>
<div>
  <form method="get" action="<%= reports_entry_report_path%>">
    <div>
      <div>
        <span>地点:
          <%= select_tag(:location_id,options_for_select(Location.all.collect{|l| [l.name,l.id]},@location_id),include_blank: false)%>
        </span>
        <span>接收时间:
          <input type="text" name="received_date_start" class="datepicker" value="<%= @received_date_start%>" />
          ~ <input type="text" name="received_date_end" class="datepicker" value="<%= @received_date_end%>" />
        </span>
      </div>
      <div>
        <input type="submit" value="查找" class="btn btn-primary">
      </div>
    </div>
  </form>
</div>
<div class="digg_pagination">
  <div class="page_info">
    <%= page_entries_info @packages%>
  </div>
  <%= will_paginate @packages, :container => false%>
</div>
<table class="table table-striped table-condensed table-hover">
  <thead>
    <tr>
      <th>零件</th>
      <th>箱数</th>
      <th>部门</th>
    </tr>
  </thead>
  <tbody>
    <%@packages.all.each do |p|%>
    <tr>
      <td><%= p.id %></td>
      <td></td>
      <td></td>
    </tr>
    <%end%>
  </tbody>
</table>
<script>
  $(function() {
  $(".datepicker").datetimepicker({
  lang: 'ch',
  format: 'Y-m-d H:i'
  });
  });
  init_check();
</script>
