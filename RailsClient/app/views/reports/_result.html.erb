<table class="table table-striped table-condensed table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>零件号</th>
    <th>数量</th>
    <th>箱数</th>
  </tr>
  </thead>
  <tbody>
  <%@packages.each_with_index do |p,index|%>
  <tr>
    <td><%= index+1%></td>
    <td><%= p.pid%></td>
    <td><%= p.count%></td>
    <td><%= p.box%></td>
  </tr>
  <%end%>
  </tbody>
</table>
