<table class="table table-striped table-condensed table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>Part Nr.</th>
    <th>BU</th>
    <th>Quantity</th>
    <th>Package Num</th>
    <th>Detail</th>
    <th>State</th>
  </tr>
  </thead>
  <tbody>
  <%@packages.each_with_index do |p,index|%>
  <tr>
    <td><%= index+1%></td>
    <td><%= p.part_id%></td>
    <td><%= p.whouse_id%></td>
    <td><%= p.total%></td>
    <td><%= p.box_count%></td>
    <td>Detail</td>
    <td><%= PackageState.display(p.state)%></td>
  </tr>
  <%end%>
  </tbody>
</table>
