<h1>Listing location_container_orders</h1>

<table>
  <thead>
    <tr>
      <th>Location container</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @location_container_orders.each do |location_container_order| %>
      <tr>
        <td><%= location_container_order.location_container_id %></td>
        <td><%= location_container_order.order_id %></td>
        <td><%= link_to 'Show', location_container_order %></td>
        <td><%= link_to 'Edit', edit_location_container_order_path(location_container_order) %></td>
        <td><%= link_to 'Destroy', location_container_order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Location container order', new_location_container_order_path %>
