<h1>Listing movement_lists</h1>

<table>
  <thead>
    <tr>
      <th>Builder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @movement_lists.each do |movement_list| %>
      <tr>
        <td><%= movement_list.builder %></td>
        <td><%= link_to 'Show', movement_list %></td>
        <td><%= link_to 'Edit', edit_movement_list_path(movement_list) %></td>
        <td><%= link_to 'Destroy', movement_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Movement list', new_movement_list_path %>
